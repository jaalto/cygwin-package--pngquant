From eff574a94dabfd91a97db543c2faf70084168c80 Mon Sep 17 00:00:00 2001
From: Jari Aalto <jari.aalto@cante.net>
Date: Fri, 20 Sep 2013 17:44:30 +0300
Subject: [PATCH] makefile
Organization: Private
Content-Type: text/plain; charset="utf-8"
Content-Transfer-Encoding: 8bit

Signed-off-by: Jari Aalto <jari.aalto@cante.net>
---
 Makefile |    4 ++--
 1 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/Makefile b/Makefile
index 7e291da..d96de29 100644
--- a/Makefile
+++ b/Makefile
@@ -41,7 +41,7 @@
 all: $(BIN)
 
 lib/libimagequant.a::
-	$(MAKE) -C lib -$(MAKEFLAGS) static
+	$(MAKE) -C lib $(MAKEFLAGS) static
 
 openmp::
 	$(MAKE) CFLAGSADD="$(CFLAGSADD) -fopenmp" OPENMPFLAGS="-Bstatic -lgomp" -j8 $(MKFLAGS)
@@ -55,7 +55,7 @@
 $(OBJS): rwpng.h build_configuration
 
 install: $(BIN)
-	install -m 0755 -p $(BIN) $(DESTDIR)$(BINPREFIX)/$(BIN)
+	install -m 0755 -p -D $(BIN) $(DESTDIR)$(BINPREFIX)/$(BIN)
 
 uninstall:
 	rm -f $(DESTDIR)$(BINPREFIX)/$(BIN)
-- 
